        <th scope="col">Descripción</th>
        <th scope="col">Acción</th>
      </tr>
    </thead>
    <tbody></tbody>
    <% if (arrayMascotas.length > 0) { %> <% arrayMascotas.forEach(mascota => {
    %>
        <tr>
        <th scope="row"> <%= mascota.id  %> </th>
        <td> <%= mascota.nombre  %> </td>
        <td> <%= mascota.descripcion  %> </td>
        <td> <%=   %> </td>
        </tr>
    <% }) %> <% } %>
  </table>
</div>

